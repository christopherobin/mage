{
	"name": "mage",
	"description": "MAGE game development platform for Node.js",
	"version": "0.13.0",
	"author": "Wizcorp K.K. <info@wizcorp.jp>",
	"homepage": "http://www.wizcorp.jp",
	"bugs": {
		"mail": "info@wizcorp.jp",
		"url": "http://github.com/Wizcorp/mage/issues"
	},
	"engines": {
		"node": "0.8.x"
	},
	"dependencies": {
		"async": "0.2.9",
		"mdns": "1.1.0",
		"zmq": "2.4.0",
		"localcache": "git+https://github.com/Wizcorp/node-localcache.git#v0.1.3",
		"multipart-parser": "0.0.2",
		"epipebomb": "0.1.1",
		"loggly": "0.3.11",
		"ws": "0.4.25",
		"graylog2": "0.0.2",
		"mysql": "2.0.0-alpha7",
		"colours": "~0.6.0",
		"memcached": "0.2.2",
		"memcached-transactions": "0.1.1",
		"rumplestiltskin": "0.0.5",
		"tomes": "0.0.15",
		"oauth": "0.9.10",
		"node-uuid": "1.4.0",
		"jsonlint": "1.6.0",
		"panopticon": "0.2.0",
		"js-yaml": "2.1.0",
		"config": "0.4.26",
		"component-builder": "~0.8.3",
		"semver": "~1.1.4",
		"mime": "1.2.9",
		"mkdirp": "~0.3.5",
		"marked": "~0.2.9",
		"highlight.js": "~7.3.0"
	},
	"main": "lib/mage",
	"devDependencies": {
		"jshint": "~2.1.3",
		"mocha": "~1.11.0"
	},
	"scripts": {
		"test": "mocha ./lib/*/test ./lib/modules/*/test --reporter spec",
		"postinstall": "./scripts/install.sh",
		"lint-all": "jshint --config ./scripts/jshint.cfg  --reporter ./scripts/humanJshintReporter.js .",
		"lint-staged": "git diff --raw --name-only --cached --diff-filter=ACMR | grep -e '\\.js$' | xargs -I '{}' jshint --config ./scripts/jshint.cfg --reporter ./scripts/humanJshintReporter.js '{}'",
		"git-setup": "./scripts/git-setup.sh"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Wizcorp/mage.git"
	},
	"private": true,
	"licences": [
		{
			"type": "Private",
			"url": "http://www.wizcorp.jp"
		}
	]
}
