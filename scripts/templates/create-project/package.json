{
  "name": "%APP_NAME%",
  "description": "%APP_DESCRIPTION%",
  "version": "%APP_VERSION%",
  "repository": "%APP_REPO%",
  "author": "%APP_AUTHOR%",
  "license": "%APP_LICENSE%",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Warning: no tests implemented yet\"",
    "start": "node . start",
    "stop": "node . stop",
    "restart": "node . restart",
    "reload": "node . reload",
    "lint-all": "jshint --config ./scripts/jshint.cfg  --reporter ./scripts/lib/humanJshintReporter.js .",
    "lint-staged": "git diff --raw --name-only --cached --diff-filter=ACMR | grep -e '\\.js$' | xargs -I '{}' jshint --config ./scripts/jshint.cfg --reporter ./scripts/lib/humanJshintReporter.js '{}'",
    "git-setup": "./scripts/setup.sh"
  },
  "engines": {
    "node": "%MAGE_NODE_VERSION%"
  },
  "dependencies": {
    "mage": "git+ssh://git@github.com:Wizcorp/mage.git#v%MAGE_VERSION%",
    "colours": "~0.6.0"
  },
  "devDependencies": {
    "jshint": "~2.1.3"
  }
}